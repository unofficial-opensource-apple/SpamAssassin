<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.updatesa</string>
	<key>Program</key>
	<string>/usr/bin/sa-update</string>
	<key>ProgramArguments</key>
	<array>
		<string>sa-update</string>
		<string>--nogpg</string>
	</array>
        <key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>2</integer>
		<key>Minute</key>
		<integer>30</integer>
	</dict>
</dict>
</plist>
